[Unit]
Description = Bugs
After=syslog.target
After=network.target

[Service]
Type=simple
User=webservices
Group=webservices
ExecStart=/usr/bin/unicorn -D -E production -c /etc/unicorn/redmine.ru /var/prologin/bugs/script/rails server webrick -e production

TimeoutSec=300

[Install]
WantedBy=multi-user.target

